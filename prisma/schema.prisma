generator client {
  provider = "prisma-client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String @id @default(cuid())
  code        Int    @unique @default(autoincrement())
  name        String
  email       String @unique
  emailHashed String
  emailTag    String
  password    String
  
  @@index([code])
  @@index([email])
  @@map("users")
}
